{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "#knexfile": ["knexfile.ts"]
    }
  },
  "include": ["src/**/*","knexfile.ts"],
  "exclude": ["node_modules", "dist"],
  
}
